{
  "browser": "chrome",
  "headless": false,
  "host": "localhost",
  "port": 3001,
  "viewport-size": "1280, 720",
  "allowed-origins": "http://localhost:5000",
  "output-dir": "./puppeteer-output",
  "save-session": true,
  "save-trace": true,
  "args": [
    "--no-sandbox",
    "--disable-setuid-sandbox",
    "--disable-dev-shm-usage",
    "--disable-accelerated-2d-canvas",
    "--no-first-run",
    "--no-zygote",
    "--disable-gpu"
  ]
} 