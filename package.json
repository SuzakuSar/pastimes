{
  "name": "summerlockIn",
  "version": "2.0.0",
  "description": "SummerLockIn - Game Platform with Enhanced Development Tools",
  "scripts": {
    "dev": "browser-sync start --proxy 'localhost:5000' --files 'website/static/**/*' --reload-delay 300",
    "sass": "sass website/static/css/scss:website/static/css --watch",
    "sass-build": "sass website/static/css/scss:website/static/css --style compressed",
    "build": "npm run sass-build",
    "start": "python app.py",
    "test": "pytest",
    "lint": "flake8 .",
    "format": "black . && isort .",
    "type-check": "mypy .",
    "quality": "npm run lint && npm run type-check && npm run format"
  },
  "devDependencies": {
    "sass": "^1.69.0",
    "browser-sync": "^2.29.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "vite": "^4.4.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0",
    "nodemon": "^3.0.0",
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "phaser": "^3.80.1",
    "socket.io-client": "^4.7.0"
  },
  "keywords": [
    "flask",
    "games",
    "leaderboard",
    "phaser",
    "web-games"
  ],
  "author": "SummerLockIn Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}